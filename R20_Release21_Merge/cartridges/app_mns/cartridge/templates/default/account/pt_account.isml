				<isbreadcrumbs bcurl1="${URLUtils.https('Account-Show')}" bctext1="My Account"/>
			</iscomment>
			<isreplace/>
		</div><!-- END: content -->
		<div class="clear"><!-- FLOAT CLEAR --></div>
		<isinclude template="components/footer/footer_checkout"/>		
	</div><!-- END: main -->
</div><!-- END: container -->
