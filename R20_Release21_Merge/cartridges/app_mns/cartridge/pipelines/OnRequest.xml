          <key-binding alias="CurrentRequest.httpProtocol +'://'+dw.system.Site.getCurrent().getCustomPreferenceValue(&quot;mobilesite&quot;)+'/' +CurrentRequest.httpProtocol +'://'+CurrentRequest.httpHost  + CurrentRequest.httpPath+'?'+CurrentRequest.httpQueryString" key="From_0"/>
