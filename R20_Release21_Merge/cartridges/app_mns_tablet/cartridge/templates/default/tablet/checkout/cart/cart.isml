										<span class="bonusitem">${Resource.msg('global.notapplied','locale',null)}</span> </isif></td>
							</tr>
						</isloop>
						</tbody>
						
					</table>
					<div class="cartfooter">
						<h2>${Resource.msg('cart.selectDeliveryOption','checkout',null)}</h2>
